<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="popups.GameExitPopUp">
   <children>
      <Text layoutX="40.0" layoutY="90.0" strokeType="OUTSIDE" strokeWidth="0.0" text="discard progress and leave the game?">
         <font>
            <Font name="Gabriola" size="28.0" />
         </font>
      </Text>
      <Button fx:id="yesBt" layoutX="97.0" layoutY="125.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="40.0" text="yes">
         <font>
            <Font name="Bauhaus 93" size="12.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="noBt" layoutX="252.0" layoutY="125.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="40.0" text="no">
         <font>
            <Font name="Bauhaus 93" size="12.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
