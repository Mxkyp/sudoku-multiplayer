<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="mainMenu" prefHeight="600.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.MainMenuController">
   <cursor>
      <Cursor fx:constant="DEFAULT" />
   </cursor>
   <children>
      <Button fx:id="single" layoutX="340.0" layoutY="370.0" mnemonicParsing="false" onAction="#playSinglePlayer" text="play">
         <font>
            <Font name="Gabriola" size="24.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="multi" layoutX="313.0" layoutY="447.0" mnemonicParsing="false" onAction="#playMultiplayer" text="multiplayer">
         <font>
            <Font name="Gabriola" size="24.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <ImageView fitHeight="25.0" fitWidth="24.0" layoutX="14.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
      <Text fx:id="mainTitle" layoutX="199.0" layoutY="276.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Sudoku!">
         <font>
            <Font name="Bauhaus 93" size="96.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
